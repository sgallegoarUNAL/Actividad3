    private void btnTRHipotenusaActionPerformed(java.awt.event.ActionEvent evt) {                                                
        double hipotenusaTR;
        int baseTR = Integer.parseInt(txtTRBase.getText());
        int alturaTR = Integer.parseInt(txtTRAltura.getText());
        TrianguloRectangulo figura1 = new TrianguloRectangulo(baseTR,alturaTR);
        hipotenusaTR = figura1.calcularArea();
        txtTRHipotenusa.setText(String.valueOf(hipotenusaTR));
    }                                               

    private void btnTRTipotrianguloActionPerformed(java.awt.event.ActionEvent evt) {                                                   
        int baseTR = Integer.parseInt(txtTRBase.getText());
        int alturaTR = Integer.parseInt(txtTRAltura.getText());
        TrianguloRectangulo figura1 = new TrianguloRectangulo(baseTR,alturaTR);
        if ((baseTR == alturaTR) && (baseTR == figura1.calcularHipotenusa()) && (alturaTR == figura1.calcularHipotenusa()))
    txtTRTipotriangulo.setText("Equilátero");
    else if ((baseTR != alturaTR) && (baseTR != figura1.calcularHipotenusa()) && (alturaTR != figura1.calcularHipotenusa()))
    txtTRTipotriangulo.setText("Escaleno");
    else
    txtTRTipotriangulo.setText("Isósceles");
    }                                                  

    private void txtTRTipotrianguloActionPerformed(java.awt.event.ActionEvent evt) {                                                   
        // TODO add your handling code here:
    }                                                  

