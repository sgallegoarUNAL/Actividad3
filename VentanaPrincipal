    private void btnAreaActionPerformed(java.awt.event.ActionEvent evt) {                                        
        
        double areaC;
        int radio = Integer.parseInt(txtCRadio.getText());
        Circulo figura1 = new Circulo(radio);
        areaC = figura1.calcularArea();
        txtCArea.setText(String.valueOf(areaC));
    }                                       

    private void btnPerimetroActionPerformed(java.awt.event.ActionEvent evt) {                                             
        double perimetroC;
        int radio = Integer.parseInt(txtCRadio.getText());
        Circulo figura1 = new Circulo(radio);
        perimetroC = figura1.calcularPerimetro();
